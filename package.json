{
    "name": "war-of-tank",
    "description": "A tank battle game built with Phaser 3",
    "version": "1.0.0",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/doanxem99/WarOfTank.git"
    },
    "bugs": {
        "url": "https://github.com/doanxem99/WarOfTank/issues"
    },
    "homepage": "https://github.com/doanxem99/WarOfTank#readme",
    "scripts": {
        "dev": "vite --config vite/config.dev.mjs",
        "build": "vite build --config vite/config.prod.mjs",
        "prettier:check": "prettier --check \"{,src/**/,webpack/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss,java}\"",
        "prettier:format": "prettier --write \"{,src/**/,webpack/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss,java}\""
    },
    "devDependencies": {
        "terser": "^5.31.0",
        "vite": "^6.2.5"
    },
    "dependencies": {
        "phaser": "^3.88.2",
        "prettier": "^3.5.3"
    }
}
